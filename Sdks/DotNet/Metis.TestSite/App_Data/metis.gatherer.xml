<?xml version="1.0" encoding="utf-8" ?>
<metis.gatherer>
  <common.setting>
    <url value="http://test.metis.huayu.nd:8881/" />
    <host value="test.metis.huayu.nd:8881" />
    <sender value="Metis.ClientSdk.Sender.TimerHttpSender, Metis.ClientSdk.Sender" />
    <debug value="true" />
    <console value="true" />
  </common.setting>

  <gatherer.apicall enabled="true">
    <extendDataPrivoder value="Metis.DemoSite.Privoder.ApiCallGathererPrivoder, Metis.DemoSite" />
    <useFilter value="true" />
    <whiteList value="v1|v2|v3" />
  </gatherer.apicall>

  <gatherer.pagevisit enabled="false">
    <extendDataPrivoder value="Metis.DemoSite.Privoder.VisitGathererPrivoder, Metis.DemoSite" />
    <useFilter value="false" />
  </gatherer.pagevisit>

  <gatherer.unhandle enabled="false">
    <extendDataPrivoder value="Metis.DemoSite.Privoder.ApiCallGathererPrivoder, Metis.DemoSite" />
    <excepts>
      <except type="DemoException" />
      <except type="BussinessException" />
    </excepts>
    <specials>
      <special type="SpecialException" level="DEBUG"/>
    </specials>
  </gatherer.unhandle>
</metis.gatherer>
